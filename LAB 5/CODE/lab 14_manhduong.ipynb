    "\n",
    "import time\n",
    "class SimpleTask:\n",
    "    def run_task(self):\n",
    "        for i in range(1,11):\n",
    "            print('Đã in lần thứ :' ,i)\n",
    "            time.sleep(2)  # dừng 2 giây giữa mỗi lần in\n",
    "def main():\n",
    "    task = SimpleTask()\n",
    "    task.run_task\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
